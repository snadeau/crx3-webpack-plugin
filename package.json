{
  "name": "crx3-webpack-plugin",
  "type": "module",
  "version": "1.0.23",
  "description": "a webpack plugin for crx3",
  "main": "dist/bundle.js",
  "repository": "https://github.com/garrettlr/crx3-webpack-plugin.git",
  "author": "Garrett Levy <Garrett@getservice.com>",
  "license": "MIT",
  "keywords": [
    "crx",
    "chrome extension",
    "chrome extension 3",
    "manifest 3",
    "chrome extension webpack plugin",
    "crx3",
    "crx3-webpack-plugin"
  ],
  "engines": {
    "node": ">=11.0.0"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production ./node_modules/.bin/rollup -c",
    "start": "cross-env NODE_ENV=development node --experimental-modules src/main.js"
  },
  "dependencies": {
    "crx3": "^1.1.2"
  },
  "devDependencies": {
    "cross-env": "^7.0.2",
    "rollup": "^1.20.3",
    "webpack": "^4.39.0"
  },
  "peerDependencies": {
    "webpack": "^4.39.0"
  }
}
